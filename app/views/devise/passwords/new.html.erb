<div class="bg-cover flex flex-col items-center justify-center min-h-screen"style="background-image: url(https://media4.giphy.com/media/Wy3Q0fuNIJmYn62Lcv/giphy.gif?cid=ecf05e47ojnl1fziiyrnf7a95mccxl0r97ldbrnu70ahz7v4&rid=giphy.gif&ct=g">
    <div class="flex items-center justify-center mb-8">
      <img src="data:image/jpeg;base64," alt="Our Clients , welcome to your knowledge platefom" class="h-16 w-16">
      <h2 class="text-2xl font-bold ml-4 text-white">Forgot your password ? </h2>
    </div>


<%= form_for(resource, as: resource_name, url: password_path(resource_name), class: "bg-white p-6 rounded-lg", html: { method: :post }) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource, class: "text-red-500 text-sm mb-4"%>


  <div class="field">
    <%= f.label :email, class: "block text-gray-700 font-medium mb-2"%><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "shadow appearance-none border border-black rounded w-fit py-2 px-3 text-gray-700 leading-tight focus:outline-none focus:shadow-outline" %>
  </div><br>

  <div class="mb-4">
    <%= f.submit "Send me reset password instructions", class:"text-center block w-full px-4 py-2 rounded-full text-white bg-black hover:bg-gray-700 focus:outline-none focus:ring-2 focus:ring-black focus:ring-opacity-50" %>
  </div>
<% end %>

<%= render "devise/shared/links" , class:"block text-gray-700 font-medium mb-2 bg-black py-2 px-4 rounded-full hover:bg-gray-700 hover:text-white"%>
</div>
</div>
